<h1>New Camper</h1>

<%= form_for @camper do |form| %>
  <p>
    <%= form.label :name %>
    <br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :age %>
    <br>
    <%= form.number_field :age %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to "Back", campers_path %>